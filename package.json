{
  "name": "@jupiterone/graph-jira",
  "version": "2.0.0",
  "description": "A JupiterOne managed integration for https://www.atlassian.com/software/jira.",
  "main": "dist/index.js",
  "repository": "https://github.com/JupiterOne/graph-jira",
  "license": "SEE LICENSE IN LICENSE",
  "files": [
    "LICENSE",
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prebuild": "yarn test",
    "start": "j1-integration collect",
    "graph": "j1-integration visualize",
    "graph:types": "j1-integration visualize-types",
    "lint": "eslint . --cache --fix --ext .ts,.tsx",
    "format": "prettier --write '**/*.{ts,js,json,css,md,yml}'",
    "type-check": "tsc",
    "test": "jest",
    "test:env": "LOAD_ENV=1 yarn test",
    "test:ci": "yarn lint && yarn type-check && yarn test",
    "build": "tsc -p tsconfig.dist.json --declaration",
    "prepush": "yarn lint && yarn type-check && jest --changedSince master",
    "prepack": "yarn build"
  },
  "dependencies": {
    "@jupiterone/integration-sdk-core": "^6.10.0",
    "dotenv": "^10.0.0",
    "jira-client": "^6.21.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.4",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-typescript": "^7.1.0",
    "@jupiterone/integration-sdk-dev-tools": "^6.10.0",
    "@jupiterone/integration-sdk-testing": "^6.10.0",
    "@types/bunyan": "^1.8.5",
    "@types/fs-extra": "^7.0.0",
    "@types/gremlin": "^3.4.2",
    "@types/jest": "^24.0.0",
    "@types/jira-client": "^6.21.0",
    "@types/nock": "^10.0.3",
    "@types/node": "~8.10.0",
    "bunyan": "^1.8.12",
    "fs-extra": "^8.0.1",
    "jest-extended": "^0.11.5",
    "lodash.merge": "^4.6.2",
    "markdown-table": "^1.1.2",
    "nock": "^10.0.6"
  }
}
